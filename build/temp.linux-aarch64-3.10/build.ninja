ninja_required_version = 1.3
cxx = c++

cflags = -Wno-unused-result -Wsign-compare -DNDEBUG -g -fwrapv -O2 -Wall -g -fstack-protector-strong -Wformat -Werror=format-security -g -fwrapv -O2 -g -fstack-protector-strong -Wformat -Werror=format-security -Wdate-time -D_FORTIFY_SOURCE=2 -fPIC -I/usr/local/lib/python3.10/dist-packages/torch/include -I/usr/local/lib/python3.10/dist-packages/torch/include/torch/csrc/api/include -I/usr/local/lib/python3.10/dist-packages/torch/include/TH -I/usr/local/lib/python3.10/dist-packages/torch/include/THC -I/usr/local/cuda/include -I/usr/include/python3.10 -c
post_cflags = -I/media/gaussian/ext/gs/hierarchical-3d-gaussians/cut_finding_sim -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1016"' -DTORCH_EXTENSION_NAME=_C -D_GLIBCXX_USE_CXX11_ABI=1 -std=c++17
cuda_dlink_post_cflags = 
ldflags = 

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc





build /media/gaussian/ext/gs/hierarchical-3d-gaussians/cut_finding_sim/build/temp.linux-aarch64-3.10/ext.o: compile /media/gaussian/ext/gs/hierarchical-3d-gaussians/cut_finding_sim/ext.cpp
build /media/gaussian/ext/gs/hierarchical-3d-gaussians/cut_finding_sim/build/temp.linux-aarch64-3.10/src/entry.o: compile /media/gaussian/ext/gs/hierarchical-3d-gaussians/cut_finding_sim/src/entry.cpp
build /media/gaussian/ext/gs/hierarchical-3d-gaussians/cut_finding_sim/build/temp.linux-aarch64-3.10/torch/torch_interface.o: compile /media/gaussian/ext/gs/hierarchical-3d-gaussians/cut_finding_sim/torch/torch_interface.cpp






